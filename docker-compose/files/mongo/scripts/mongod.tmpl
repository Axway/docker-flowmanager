# mongod.conf

# Where and how to store data.
storage:
  dbPath: /data/db
  journal:
    enabled: true
  engine: wiredTiger
  directoryPerDB: true

# How the process runs
processManagement:
  timeZoneInfo: /usr/share/zoneinfo

# Network
net:
  bindIp: $BIND_IP

# Security
security:
  authorization: enabled
